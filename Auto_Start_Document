nano /home/jetson/Documents/AI/Auto_start.desktop


[Desktop Entry]
Name=Dimples Vision System
Exec=/usr/bin/python3 /home/jetson/Documents/AI/Pins_VS/Main_01.py
Type=Application
StartupNotify=false
Terminal=false

sudo mv /home/jetson/Documents/AI/Auto_start.desktop /etc/xdg/autostart/

sudo chmod +x /etc/xdg/autostart/Auto_start.desktop

ls -l /etc/xdg/autostart/Auto_start.desktop


/usr/bin/python3 /home/jetson/Documents/AI/Pins_VS/Main_01.py

sudo reboot

